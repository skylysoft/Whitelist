local HWID = nil
local HWIDPlr = game:GetService("RbxAnalyticsService"):GetClientId()
local key = _G.Key
local check = "https://skylyhubwhitelist.000webhostapp.com/check.php?key=" .. key
local KeyAndHWID = false
local ScriptIsRunning = false
--------------------------------------------------------------------------------------------------------
function Whitelist1()
	if game:HttpGet(check) == "Whitelisted" then
	print("pass")
	HWID = "1C973ADB-8DF9-421C-9C84-819A23D7608A"
		else
			game.Players.LocalPlayer:Kick("Invalid Key! Please Buy Script By Komi Shouko#9006.")
	end

	if HWID == HWIDPlr then
		print("pass2")
		
		KeyAndHWID = true
	else
		print("incorract2")
	end
end
Whitelist1()

if KeyAndHWID == true then
	print("pass3")
	ScriptIsRunning = true
end

if ScriptIsRunning == true then
	print("pass4")
	print("-----------------------")
	return
end
